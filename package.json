{
    "name": "fookie",
    "private": true,
    "packageManager": "npm@10.2.4",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "test": "turbo run test",
        "test:watch": "vitest",
        "publish-packages": "turbo run build && npm publish --workspaces --access public",
        "lint": "turbo run lint",
        "format": "turbo run format"
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "eslint": "^8.0.0",
        "prettier": "^3.0.0",
        "turbo": "latest"
    }
}
